class Solution {
    public int minOperations(int[] nums, int k) {
        int n=nums.length;
        int sum=0;
        int ops=0;
        for(int num : nums)
        {
            sum+=num;
        }
        if(sum<k)
        {
            return sum;
        }
        else{
            ops=sum%k;
        }
        return ops;
    }
}
